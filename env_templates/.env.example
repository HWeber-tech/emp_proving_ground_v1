# Runtime configuration
RUN_MODE=paper
CONFIRM_LIVE=false
EMP_KILL_SWITCH=/tmp/emp_pg.KILL
EMP_ENVIRONMENT=demo
CONNECTION_PROTOCOL=fix
EMP_TIER=tier_0

# Broker credentials (leave blank for paper)
ICMARKETS_ACCOUNT=
ICMARKETS_PASSWORD=

# Tier-0 ingest defaults
# Comma-separated symbols for Yahoo ingest
SYMBOLS=EURUSD,GBPUSD
# DuckDB path for storing ingested data
DUCKDB_PATH=data/tier0.duckdb
# Optional encrypted volume root for DuckDB storage
EMP_DUCKDB_ENCRYPTED_ROOT=
# Require the encrypted root to be used (set to true/1 to enforce)
EMP_REQUIRE_ENCRYPTED_DUCKDB=false
